{
  "name": "spryker/rabbit-mq",
  "description": "Spryker RabbitMq adapter",
  "type": "library",
  "license": "MIT",
  "keywords": ["framework", "rabbit-mq", "queue", "standards"],
  "homepage": "http://spryker.com",
  "authors": [
    {
      "name": "Spryker",
      "homepage": "http://spryker.com"
    }
  ],
  "support": {
    "source": "https://github.com/spryker/rabbit-mq"
  },
  "require": {
    "php": ">=5.4.16",
    "php-amqplib/php-amqplib": "^2.6",
    "spryker/kernel": "^3.0.0",
    "spryker/queue": "^1.0.0",
    "spryker/transfer": "^3.3.4"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spryker\\Test\\": "tests"
    }
  },
  "scripts": {
    "cs-check": "phpcs --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -v --ignore=/vendor/,/templates/ ./",
    "cs-fix": "phpcbf --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -v --ignore=/vendor/,/templates/ ./"
  },
  "include-path":  ["src/"],
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ]
}
